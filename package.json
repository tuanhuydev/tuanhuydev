{
  "name": "tuanhuydev",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "npm run db:generate && next dev",
    "build": "npm run db:generate && next build",
    "start": "next start",
    "lint": "next lint && npx prettier --write .",
    "test": "npx jest",
    "db:schema:pull": "npx prisma db pull --schema ./backend/database/schema.prisma",
    "db:schema:push": "npx prisma db push --schema ./backend/database/schema.prisma",
    "db:migrate:run": "npx prisma migrate deploy --schema ./backend/database/schema.prisma",
    "db:migrate:create": "prisma migrate dev --schema ./backend/database/schema.prisma --name",
    "db:generate": "npx prisma generate --schema ./backend/database/schema.prisma",
    "db:seed": "npx prisma db seed",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./backend/database/seeders/seeder.ts",
    "prismaPath": "./node_modules/.prisma"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@aws-sdk/client-s3": "^3.363.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@prisma/client": "^4.16.2",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.5.2",
    "aws-sdk": "^2.1409.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "draft-js": "^0.11.7",
    "formidable": "^1.2.2",
    "framer-motion": "^9.0.7",
    "husky": "^8.0.3",
    "i18next": "^23.2.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.0",
    "next": "12.3.0",
    "next-i18next": "^14.0.0",
    "npm": "^9.7.1",
    "prisma": "^4.16.2",
    "qs": "^6.11.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^7.44.3",
    "react-i18next": "^13.0.1",
    "react-quill": "^2.0.0",
    "react-redux": "^8.0.7",
    "redux-saga": "^1.2.3",
    "sass": "^1.62.1",
    "sharp": "^0.31.3",
    "tailwindcss": "^3.2.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/formidable": "^3.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.7.18",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.13",
    "@types/react-dom": "18.0.6",
    "@types/react-draft-wysiwyg": "^1.13.4",
    "autoprefixer": "^10.4.13",
    "commitlint": "^17.4.4",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-environment-jsdom": "^29.4.3",
    "postcss": "^8.4.21",
