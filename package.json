{
  "name": "tuanhuydev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:generate && next dev --turbo",
    "build": "npm run db:generate && next build",
    "start": "next start",
    "lint": "next lint && npx prettier --write .",
    "test": "npx jest --verbose",
    "db:schema:pull": "npx prisma db pull",
    "db:schema:push": "npx prisma db push",
    "db:migrate:run": "npx prisma migrate deploy",
    "db:migrate:create": "prisma migrate dev --name",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seeders/seeder.ts",
    "prismaPath": "./node_modules/.prisma"
  },
  "browserslist": [
    "chrome 64",
    "edge 79",
    "firefox 67",
    "opera 51",
    "safari 12"
  ],
