{
  "name": "tuanhuydev",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && npx prettier --write .",
    "test": "npx jest",
    "prepare": "husky install",
    "release": "standard-version",
    "db:schema": "npx prisma db pull --schema ./backend/database/schema.prisma",
    "db:generate": "npx prisma generate --schema ./backend/database/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:migration": "npx prisma migrate dev",
    "release:minor": "npm run release --release-as minor",
    "release:patch": "npm run release --release-as patch",
    "release:major": "npm run release --release-as major"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./backend/database/seeders/seeder.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "eslint-config-prettier": "^8.8.0",
    "framer-motion": "^9.0.7",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "next": "12.3.0",
    "prisma": "^4.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.58.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "commitlint": "^17.4.4",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-environment-jsdom": "^29.4.3",
    "postcss": "^8.4.21",
    "prettier": "2.8.7",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3"
  }
}
