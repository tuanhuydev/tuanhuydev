{
	"family": "tuanhuydev-task",
	"containerDefinitions": [
		{
			"name": "app",
			"image": "tuanhuydev/tuanhuydev:latest",
			"cpu": 0,
			"portMappings": [
				{
					"name": "app-3000-tcp",
					"containerPort": 3000,
					"hostPort": 80,
					"protocol": "tcp"
				}
			],
			"essential": true
		}
	],
	"networkMode": "bridge",
	"cpu": "1024",
	"memory": "512"
}
